package backjoon;

import java.util.Scanner;

public class silsuep6 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		if(a%2==1) {
			for (int i = 0; i < a; i+=2) {
				System.out.print("*");
				if(i==a-1) {
					break;
				}
				System.out.print(" ");
			}
		}else {
			for (int i = 0; i < a; i++) {
				System.out.print("*");
				if(i==a-1) {
					break;
				}
				System.out.print(" ");
			}
		}
	}
}
