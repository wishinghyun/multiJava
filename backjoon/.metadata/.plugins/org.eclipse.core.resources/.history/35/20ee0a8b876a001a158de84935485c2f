package backjoon;

import java.util.Arrays;
import java.util.Scanner;

public class array1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int[] list = new int[a];
		for (int i = 0; i < list.length; i++) {
			list[i] = sc.nextInt();
		}
		Arrays.sort(list);
		System.out.println(list[0]+" "+list[a]);
	}

}
